import java.util.Scanner;

public class ex02 {

	public String Fibonacci(int num) {
		int anterior = 0, atual = 1, proximo = 0;
		while (num > atual) {
			proximo = atual + anterior;
			anterior = atual;
			atual = proximo;
		}
		if (num == 1 || num == 0) {
			return num + "Pertence a sequência de números Fibonacci";
		}
		if (num == atual) {
			return num + "Pertence a sequência de números Fibonacci";
		} else if (num < atual) {
			return num + "Não pertence a sequência de números Fibonacci";
		}
		return null;
	}

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int num;
		try {
			System.out.println("Informe um número inteiro");
			if (scan.hasNext()) {
				num = scan.nextInt();
				System.out.println(num);
			}

		} catch (Exception e) {
			System.out.println("Número informado inválido!");
		}

	}

}
